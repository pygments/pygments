'#'           Comment.Preproc
'define\tQAPPEND(text, tlen)\tdo {\t\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tchar *prev_text = asn1p_lval.tv_opaque.buf;\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tint prev_len = asn1p_lval.tv_opaque.len;\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tchar *p;\t\t\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\t\t\t\t\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tp = malloc((tlen) + prev_len + 1);\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tif(p == NULL) return -1;\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\t\t\t\t\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tif(prev_text) memcpy(p, prev_text, prev_len);\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tmemcpy(p + prev_len, text, tlen);\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

"\t\tp[prev_len + (tlen)] = '\\0';\t\t\t\\" Comment.Preproc
'\n'          Comment.Preproc

'\t\t\t\t\t\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tfree(asn1p_lval.tv_opaque.buf);\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tasn1p_lval.tv_opaque.buf = p;\t\t\t\\' Comment.Preproc
'\n'          Comment.Preproc

'\t\tasn1p_lval.tv_opaque.len = (tlen) + prev_len;\t\\' Comment.Preproc
'\n'          Comment.Preproc

