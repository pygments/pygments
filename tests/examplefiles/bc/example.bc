/*
 * Calculate the Greatest Common Divisor of a and b.
 */
define gcd(a, b) {
    auto tmp;

    /*
     * Euclidean algorithm
     */
    while (b != 0) {
        tmp = a % b;
        a = b;
        b = tmp;
    }
    return a;
}
