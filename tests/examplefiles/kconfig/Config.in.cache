config FREETZ_TYPE_FON_WLAN_7270
	depends on \
		FREETZ_TYPE_FON_WLAN_7270_V1 || \
		FREETZ_TYPE_FON_WLAN_7270_V2 || \
		FREETZ_TYPE_FON_WLAN_7270_V3
